document.addEventListener("DOMContentLoaded", function () {
    const carousel = document.querySelector('.scroll-pdp');
    const column = carousel.parentElement;

    let currentTop = 0;
    let targetTop = 0;

    function adjustCarouselPosition() {
        const columnTop = column.offsetTop;
        const columnBottom = columnTop + column.offsetHeight - carousel.offsetHeight;
        const viewportWidth = window.innerWidth || document.documentElement.clientWidth;

        if (viewportWidth >= 768) {
            const scrollPosition = window.scrollY || window.pageYOffset;

            if (scrollPosition > columnTop && scrollPosition < columnBottom) {
                targetTop = scrollPosition - columnTop;
                carousel.style.position = 'absolute';
                carousel.style.width = column.offsetWidth + 'px';
            } else if (scrollPosition >= columnBottom) {
                targetTop = columnBottom - columnTop;
                carousel.style.width = '100%';
            } else {
                targetTop = 0;
                carousel.style.width = '100%';
            }

            requestAnimationFrame(smoothScroll);
        } else {
            carousel.style.position = 'static';
            carousel.style.width = '100%';
        }
    }

    function smoothScroll() {
        const diff = targetTop - currentTop;

        if (Math.abs(diff) < 1) {
            carousel.style.top = targetTop + 'px';
            currentTop = targetTop;
        } else {
            currentTop += diff * 0.1;
            carousel.style.top = currentTop + 'px';
            requestAnimationFrame(smoothScroll);
        }
    }

    window.addEventListener('scroll', adjustCarouselPosition);
    window.addEventListener('resize', adjustCarouselPosition);
});</script>
