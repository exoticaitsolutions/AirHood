<script>
document.addEventListener('DOMContentLoaded', function() {
  const colorSelector = document.getElementById('color-selector');
  const selectedVariant = document.getElementById('selected-variant');

  // Initialize MutationObserver
  const observer = new MutationObserver(function(mutations) {
    mutations.forEach(function(mutation) {
      if (mutation.attributeName === 'checked') {
        const radio = mutation.target;
        if (radio.checked) {
          selectedVariant.value = radio.value;
        }
      }
    });
  });

  // Attach MutationObserver to each radio button to observe changes in attributes
  const radios = colorSelector.querySelectorAll('input[type="radio"]');
  radios.forEach((radio) => {
    observer.observe(radio, { attributes: true });
    
    // Initialize the selectedVariant value if a radio button is already checked
    if (radio.checked) {
      selectedVariant.value = radio.value;
    }
  });
});

  document.addEventListener("DOMContentLoaded", function() {
    var swatches = document.querySelectorAll('.pf-vs-square');
    if (swatches.length > 1) {
      document.querySelector('#color-selector').style.display = 'block';
    }
  });
</script>

