{% assign collect_phone_number_signup = true %}
{% assign newsletter_form_submit = 'Sign Up' %}
{% assign redirect_after_signup_page = 'thank-you' %}

{% assign newsletter_title = 'Hurry, <span class="green-color">Crowdfunding  Soon</span>' %}
{% assign newsletter_text = 'Sign up now to access the launch-day deal of up to ' %}
{% assign newsletter_text_discount = '20% off' %}
{% assign privacy_text = 'We respect your privacy' %}

{% assign redirect_after_signup_message = 'Redirecting please wait...' %}
{% assign redirect_after_signup_message_subscribed = 'Already subscribed, redirecting...' %}
