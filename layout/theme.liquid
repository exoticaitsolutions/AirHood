<script>
$(document).ready(function() {
// Function to remove the keychain product from the cart if it's the only item
function checkAndRemoveKeychainProduct() {
  var keychainProductId = $('#keychain-product-id').val();

  $.ajax({
    type: 'GET',
    url: '/cart.js',
    dataType: 'json',
    success: function(cart) {
      if (cart.item_count === 1 && cart.items[0].variant_id == keychainProductId) {
        // If the cart contains only the keychain product, simulate click on the remove button
        $('.item[data-variant-id="' + keychainProductId + '"] .remove').click();
      }
    },
    error: function(xhr, status, error) {
      console.error('Error checking cart:', error);
    }
  });
}

// Initial check when the page loads
checkAndRemoveKeychainProduct();

// Periodically check the cart contents and remove the keychain product if necessary
setInterval(checkAndRemoveKeychainProduct, 2000); // Check every 5 seconds (adjust as needed)

 });

$(document).ready(function() {
 // First button click event
  $('body').on('click', '#signup_button', function(e) {
    e.preventDefault();

    var email = $('#contact_email').val();
    var keychainProductId = $('#keychain-product-id').val();

    if (email.trim() === '') {
      alert('Email field is empty. Please enter your email.');
      return; // Stop execution if the email is empty
    }

    // Check if the keychain product is already in the cart
    if (isProductInCart(keychainProductId)) {
      alert('The Free Airhood Keychain product is already in your cart.');
      return; // Stop execution if the keychain product is already in the cart
    }

    // Add product to cart
    $.ajax({
      type: 'POST',
      url: '/cart/add.js',
      data: {
        quantity: 1,
        id: keychainProductId
      },
      dataType: 'json',
      success: function() {
        console.log('Product added to cart');
      var message = $('<div>').text('Free Airhood Keychain product added to cart successfully.').css({
        'background-color': '#da281b',
        'color': 'white',
        'padding': '10px',
        'position': 'absolute',
        'top': '22px',
        'left': '45%',
        'z-index': '9999999999',
        'transform': 'translate(-50%, -50%)',
        'border-radius': '5px',
        'opacity': '1', // Ensure the message is fully visible
        'transition': 'none' // Disable any transition effects
    });

    $('body').append(message);

    // Remove the message after 3 seconds
    setTimeout(function() {
        message.remove();
    }, 5000);

        // Submit the default Shopify form to create customer
        $('#contact_form').submit();

        // Set cookie after product added to cart and customer added
        setCookie('cart_drawer_open', 'true', 1);
      },
      error: function(xhr, status, error) {
        console.error('Error adding product to cart:', error);
        alert('Error adding the product to your cart. Please try again later.');
      }
    });
  });

 
  // Function to check if a product is in the cart
  function isProductInCart(productId) {
    var isInCart = false;
    $.ajax({
      type: 'GET',
      url: '/cart.js',
      dataType: 'json',
      async: false, // Ensure synchronous request to wait for the response
      success: function(cart) {
        cart.items.forEach(function(item) {
          if (item.variant_id == productId) {
            isInCart = true;
          }
        });
      },
      error: function(xhr, status, error) {
        console.error('Error checking cart:', error);
      }
    });
    return isInCart;
  }

  // Second button click event
  $('body').on('click', '#signup_button_auto', function(e) {
    e.preventDefault();

    var email = $('#contact_email').val();

    if (email.trim() === '') {
      alert('Email field is empty. Please enter your email.');
      return; // Stop execution if the email is empty
    }

    // Trigger the form submission
    $('#contact_form').submit();
  });

  setInterval(function() {
    var url = window.location.href;
    if ((jQuery('.slidecarthq-overlay').length) && ((url.indexOf('form_type=customer') > -1) || (url.indexOf('customer_posted') > -1))) {
      var cartDrawerOpens = getCookie('cart_drawer_open');
      if (cartDrawerOpens == 'true') {
        var newUrl = url.split('?')[0];
        $('.cart-icon').click();
        setCookie('cart_drawer_open', 'false', 1);
        window.history.pushState({ path: newUrl }, '', newUrl);
      }
    }
  }, 1000);

  function getCookie(name) {
    var value = '; ' + document.cookie;
    var parts = value.split('; ' + name + '=');
    if (parts.length == 2) return parts.pop().split(';').shift();
  }

  function setCookie(name, value, days) {
    var expires = '';
    if (days) {
      var date = new Date();
      date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
      expires = '; expires=' + date.toUTCString();
    }
    document.cookie = name + '=' + value + expires + '; path=/';
  }
});

</script>
