  <script>
    function detectOverflowX() {
    let overflowingElements = [];
        function checkOverflow(element) {
          if (element.scrollWidth > element.clientWidth) {
                overflowingElements.push(element);
            }

            for (let child of element.children) {
                checkOverflow(child);
            }
        }

        checkOverflow(document.body);

        if (overflowingElements.length) {
            console.warn('Detected elements with overflow-x issues:');
            overflowingElements.forEach((el, index) => {
                console.log(`[${index + 1}]`, el);
            });
        } else {
            console.log('No elements with overflow-x issues found.');
        }
    }

    detectOverflowX();
  </script>
