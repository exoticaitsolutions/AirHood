              $('head script[src*="shopify"]').each(function() {
                var script = document.createElement('script');
                script.type = 'text/javascript';
                script.src = $(this).attr('src');
                $('body').append(script);
              });
              $('body').removeClass('fancybox-active');
              $.fancybox.destroy();
              InstantClick.init();
